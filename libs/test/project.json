{
  "projectType": "library",
  "sourceRoot": "libs/test/src",
  "prefix": "test",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/test"],
      "options": {
        "jestConfig": "libs/test/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/test/**/*.ts", "libs/test/**/*.html"]
      }
    }
  },
  "tags": ["type:test"]
}
